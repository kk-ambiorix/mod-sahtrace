-include ../makefile.inc

# Set default values if CONFIG_SAH_MOD_SAHTRACE_DEFAULT_TRACE_ZONE_LEVEL or CONFIG_SAH_MOD_SAHTRACE_DEFAULT_LOG_LEVEL is empty
ifeq ($(strip $(CONFIG_SAH_MOD_SAHTRACE_DEFAULT_TRACE_ZONE_LEVEL)),)
    CONFIG_SAH_MOD_SAHTRACE_DEFAULT_TRACE_ZONE_LEVEL := 200
endif

ifeq ($(strip $(CONFIG_SAH_MOD_SAHTRACE_DEFAULT_LOG_LEVEL)),)
    CONFIG_SAH_MOD_SAHTRACE_DEFAULT_LOG_LEVEL := 200
endif

all:
	m4 -D DEFAULT_TRACE_ZONE_LEVEL=$(CONFIG_SAH_MOD_SAHTRACE_DEFAULT_TRACE_ZONE_LEVEL) -D DEFAULT_LOG_LEVEL=$(CONFIG_SAH_MOD_SAHTRACE_DEFAULT_LOG_LEVEL) global_trace.odl.m4 > global_trace.odl
clean:
	rm -rf global_trace.odl

.PHONY: all clean
